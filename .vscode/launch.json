    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "file-download-service (one time exec)",
            "program": "${workspaceFolder}/fileDownloadService/index.js",
            "cwd": "${workspaceFolder}/fileDownloadService",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "file-storage-tester",
            "program": "${workspaceFolder}/fileStorageTester/index.js",
            "cwd": "${workspaceFolder}/fileStorageTester",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}